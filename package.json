{
  "name": "n8n-nodes-evolution-api-lite",
  "version": "1.0.2",
  "description": "N8N Community Node for Evolution API - Lite Version",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "JoÃ£o Felipe Cardoso Santos",
    "email": "jfcardososantos@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfcardososantos/n8n-nodes-evolution-api-lite.git"
  },
  "homepage": "https://github.com/jfcardososantos/n8n-nodes-evolution-api-lite",
  "bugs": {
    "url": "https://github.com/jfcardososantos/n8n-nodes-evolution-api-lite/issues"
  },
  "engines": {
    "node": ">=18.10"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "version:patch": "npm version patch && git push && git push --tags",
    "version:minor": "npm version minor && git push && git push --tags",
    "version:major": "npm version major && git push && git push --tags",
    "release": "npm run build && npm publish",
    "release:patch": "npm run version:patch && npm run release",
    "release:minor": "npm run version:minor && npm run release",
    "release:major": "npm run version:major && npm run release",
    "release:auto": "release-it",
    "release:auto:patch": "release-it patch",
    "release:auto:minor": "release-it minor",
    "release:auto:major": "release-it major"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EvolutionApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/EvolutionApi/EvolutionApi.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@typescript-eslint/parser": "^8.27.0",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "n8n-workflow": "^1.70.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "release-it": "^16.1.3",
    "@release-it/conventional-changelog": "^7.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "axios": "^1.8.4"
  },
  "resolutions": {
    "axios": "^1.8.4"
  }
}
